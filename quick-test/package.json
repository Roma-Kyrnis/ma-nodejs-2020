    "node-telegram-bot-api": "^0.40.0",
    "pg": "^7.18.1",
    "telegraf": "^3.36.0",
    "telegraf-flow": "^8.3.0"
  },
  "author": "Roman",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "prettier": "1.19.1"
  }
